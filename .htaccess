RewriteEngine On
RewriteBase /

RewriteRule ^home/?$ /views/home/home.php [L]

RewriteRule ^login/?$ /views/login/login.php [L]

RewriteRule ^register/?$ /views/register/register.php [L]

RewriteRule ^posts/?$ /views/posts/posts.php [L]

RewriteRule ^post/([0-9]+)/?$ /views/post/post.php?id=$1 [L]



